{
  "name": "el-form-core",
  "version": "2.1.0",
  "description": "Framework-agnostic form validation engine - schema-first validation core for TypeScript applications. Supports Zod, Yup, Valibot and custom validators.",
  "author": "Nichalas Barnes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colorpulse6/el-form"
  },
  "homepage": "https://elform.dev/",
  "keywords": [
    "form",
    "validation",
    "zod",
    "yup",
    "valibot",
    "typescript",
    "framework-agnostic",
    "schema-validation",
    "type-safe",
    "form-validation",
    "schema-first",
    "validation-engine",
    "typescript-validation",
    "form-library",
    "validator",
    "schema-based-validation"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {},
  "peerDependencies": {
    "zod": "^3.22.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "vitest": "^2.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
